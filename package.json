{
  "name": "@teamantware/node-hirehop",
  "version": "1.0.0",
  "description": "An unofficial Hirehop SDK for node",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
